{
  "name": "cms-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0",
    "build": "ng build --prod",
    "ci": "ng test --watch=false --browsers=ChromeHeadless && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib:copy:npmrc": "copy ./docker-npmrc ./dist/@aviato/components",
    "e2e:localhost": "npm-run-all -s \"e2e:config -- --BASE_URL=http://localhost:4200 --setup=local\"",
    "e2e:ite": "npm-run-all -s \"e2e:config -- --BASE_URL=https://cafetownsend-angular-rails.herokuapp.com/login --setup=prod\"",
    "e2e:ute": "npm-run-all -s \"e2e:config -- --BASE_URL=https://cms-ute.klm.com --setup=prod\"",
    "e2e:ae": "npm-run-all -s \"e2e:config -- --BASE_URL=https://cms-ae.klm.com --setup=prod\"",
    "e2e:config": "protractor ./e2e/cucumber/config/protractor.conf.js --disableChecks",
    "e2e:hub": "protractor ./e2e/cucumber/config/protractor.hub.conf.js",
    "e2e:report": "node ./e2e/cucumber/config/generateReports.js",
    "webdriver:start": "webdriver-manager start",
    "webdriver:update": "webdriver-manager update",
    "webdriver:clean": "webdriver-manager clean",
    "webdriver:stop": "webdriver-manager shutdown"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "^7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.2.0",
    "@angular/platform-browser": "^7.1.4",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@types/hammerjs": "^2.0.36",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "mocha": "^6.2.0",
    "moment": "^2.24.0",
    "ngx-ckeditor": "^0.4.0",
    "ngx-toastr": "^9.1.2",
    "nyc": "^13.3.0",
    "puppeteer": "^1.20.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.4",
    "@angular/cli": "~7.2.0",
    "@angular/compiler": "^7.2.7",
    "@angular/compiler-cli": "^7.2.7",
    "@angular/language-service": "~7.2.0",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.1",
    "@types/cucumber": "^4.0.7",
    "@types/jasmine": "~3.3.5",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.12.26",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codelyzer": "~4.5.0",
    "cucumber": "^5.1.0",
    "cucumber-html-report": "^0.6.5",
    "cucumber-html-reporter": "^5.0.0",
    "cucumber-pretty": "^1.5.2",
    "cucumberjs-allure-reporter": "^1.0.3",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-mocha": "^1.3.0",
    "mkdirp": "^0.5.1",
    "multiple-cucumber-html-reporter": "^1.12.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "package.json": "^2.0.1",
    "protractor": "~5.4.0",
    "protractor-cucumber-framework": "^6.1.3",
    "sync-request": "^6.1.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.12.0",
    "typescript": "~3.2.2",
    "yargs": "^13.3.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.0.1"
  }
}
